{
  "name": "aura-backend",
  "version": "0.0.1",
  "description": "Backend for aura (vocnea)",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "$(npm bin)/nodemon server.js --ignore tools --ignore build --ignore analysis",
    "lint": "$(npm bin)/eslint .",
    "protobuf:compile": "$(npm bin)/pbjs protocol/Dataset.proto -t static-module -w es6 --es6 -o ./protocol.js",
    "docker:build": "docker build . -t registry.aura.rest/edge",
    "docker:push": "docker push registry.aura.rest/edge",
    "docker:deploy": "docker build . -t registry.aura.rest/edge && docker push registry.aura.rest/edge"
  },
  "repository": {
    "url": "ssh://aura-development@vs-ssh.visualstudio.com:22/Aura/_ssh/backend",
    "type": "git"
  },
  "author": "Moritz Hoffmann <mhoffmann@teco.edu>",
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "2",
    "amqplib": "^0.5.2",
    "config": "^1.30.0",
    "content-type": "^1.0.4",
    "eslint_d": "^6.0.1",
    "global": "^4.3.2",
    "jwks-rsa": "^1.3.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-jwt": "^3.3.2",
    "koa-logger": "^3.2.0",
    "koa-protobuf": "^0.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "md5": "^2.2.1",
    "mongoose": "^5.2.4",
    "pretty-bytes": "^5.1.0",
    "protobufjs": "^6.8.8",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "nodemon": "^1.18.3"
  }
}
